{
  "framework": null,
  "outputDirectory": ".",
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "routes": [
    {
      "src": "^/.well-known/(.*)",
      "dest": "/.well-known/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}